    "section_rich_text_hYVVym": {
      "type": "section-rich-text",
      "blocks": {
        "heading_WHwqBe": {
          "type": "heading",
          "settings": {
            "title": "Our Customer Experience Team Is Here For You!",
            "text_font": 40,
            "padding_bottom": 30
          }
        },
        "text_CHxcpD": {
          "type": "text",
          "settings": {
            "text": "<p>Tiege Hanley tries to cut out all the BS associated with men’s skin care and that extends to our customer service. If you have an issue, we got you!<\/p>",
            "text_font": 16,
            "text_style": "font-body",
            "padding_bottom": 30
          }
        },
        "text_6dzKNQ": {
          "type": "text",
          "settings": {
            "text": "<ul><li> <strong>Need help with your account?<\/strong> We’ll help you fix it!<\/li><li> <strong>Trouble with your delivery?<\/strong> No hassle reships!<\/li><li> <strong>Weren’t ready for your next box?<\/strong> Worry free returns!<\/li><\/ul>",
            "text_font": 16,
            "text_style": "font-body",
            "padding_bottom": 16
          }
        },
        "text_aztUdc": {
          "type": "text",
          "settings": {
            "text": "<p>If you reach us during business hours you can expect a response within an hour! All inquiries will be responded to within 24 hours, guaranteed.<\/p>",
            "text_font": 16,
            "text_style": "font-body",
            "padding_bottom": 0
          }
        }

    "section_rich_text_3NDRYN": {
      "type": "section-rich-text",
      "blocks": {
        "heading_d43DHF": {
          "type": "heading",
          "settings": {
            "title": "Our Commitment",
            "text_font": 40,
            "padding_bottom": 16
          }
        },
        "text_Wb9PBG": {
          "type": "text",
          "settings": {
            "text": "<p>Tiege Hanley feels that it can make the biggest impact by committing to a closed-loop recycling program. Doing so will limit the amount of new waste we contribute to the environment.<\/p>",
            "text_font": 16,
            "text_style": "font-body",
            "padding_bottom": 16
          }
        }
      },
      "block_order": [
        "heading_d43DHF",
        "text_Wb9PBG"
      ],
      "custom_css": [
